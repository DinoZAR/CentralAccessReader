{
  "name": "central-access-reader",
  "version": "0.1.0",
  "description": "Text-to-speech reader that reads math well.",
  "main": "build/main.js",
  "scripts": {
    "start": "webpack --config webpack.config.main.js electron && electron .",
    "web": "webpack-dev-server --config webpack.config.renderer.js",
    "test": "jest",
    "build": "electron-build-env neon build",
    "lint": "eslint ./ui ./src ./main.js"
  },
  "jest": {
    "setupFiles": [
      "./setupJest.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./__mocks__/fileMock.js",
      "\\.(css|less)$": "./__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DinoZAR/CentralAccessReader.git"
  },
  "keywords": [
    "text-to-speech",
    "TTS"
  ],
  "author": "Spencer Graffe",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/DinoZAR/CentralAccessReader/issues"
  },
  "homepage": "https://github.com/DinoZAR/CentralAccessReader#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "electron": "^1.8.4",
    "electron-build-env": "^0.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "jest": "^22.4.3",
    "neon-cli": "^0.1.22",
    "node-loader": "^0.6.0",
    "style-loader": "^0.20.3",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "aphrodite": "^2.1.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-thunk": "^2.2.0",
    "temp": "^0.8.3"
  }
}
